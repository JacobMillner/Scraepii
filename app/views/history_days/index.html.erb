<p id="notice"><%= notice %></p>

<h1>Listing History Days</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Open</th>
      <th>Close</th>
      <th>High</th>
      <th>Low</th>
      <th>Volume</th>
      <th>Points</th>
      <th>Weekofyear</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @history_days.each do |history_day| %>
      <tr>
        <td><%= history_day.date %></td>
        <td><%= history_day.open %></td>
        <td><%= history_day.close %></td>
        <td><%= history_day.high %></td>
        <td><%= history_day.low %></td>
        <td><%= history_day.volume %></td>
        <td><%= history_day.points %></td>
        <td><%= history_day.weekOfYear %></td>
        <td><%= link_to 'Show', history_day %></td>
        <td><%= link_to 'Edit', edit_history_day_path(history_day) %></td>
        <td><%= link_to 'Destroy', history_day, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New History day', new_history_day_path %>
