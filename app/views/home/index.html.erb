<div class="row">
  <div class="col-lg-10 col-centered">
    <div class="jumbotron">
      <H3><%= @title %></H3>
      Total Records: <p class="text-primary"><%= @stuff.count %></p>
    </div>
  </div>
</div>
<div class="row">
  <div class="col-lg-8 col-centered well well-xs">
    <table class="table table-striped table-hover">
      <thead>
        <tr class="info">
          <td>Date</td>
          <td>Open</td>
          <td>High</td>
          <td>Low</td>
          <td>Close</td>
          <td>Volume</td>
        </tr>
      </thead>
      <% @stuff.drop(1).each do |hist| -%>
       <tr>
          <td><%= hist.children[1].text %></td>
          <td><%= hist.children[2].text %></td>
          <td><%= hist.children[3].text %></td>
          <td><%= hist.children[4].text %></td>
          <td><%= hist.children[5].text %></td>
          <td><%= hist.children[6].text %></td>
        </tr>
      <% end -%>
    </table>
  </div>
</div>